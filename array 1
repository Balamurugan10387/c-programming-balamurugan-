#include <stdio.h>

int main() {
    int arr[10][10];
    int rows, cols, i, j;

    printf("Enter number of rows and columns: ");
    scanf("%d %d", &rows, &cols);

    printf("Enter elements of the array:\n");
    for(i = 0; i < rows; i++) {
        for(j = 0; j < cols; j++) {
            scanf("%d", &arr[i][j]);
        }
    }

    // Find maximum element in each column
    for(j = 0; j < cols; j++) {
        int max = arr[0][j];  // Assume first element of column is max

        for(i = 1; i < rows; i++) {
            if(arr[i][j] > max) {
                max = arr[i][j];
            }
        }

        printf("Maximum element in column %d = %d\n", j + 1, max);
    }

    return 0;
}
