#include <stdio.h>

int main() {
    int rows, cols;

    // Input rows and columns
    printf("Enter number of rows and columns: ");
    scanf("%d %d", &rows, &cols);

    int arr[rows][cols];

    // Input matrix elements
    printf("Enter matrix elements:\n");
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < cols; j++) {
            scanf("%d", &arr[i][j]);
        }
    }

    // Find maximum in each column
    printf("Maximum element in each column:\n");

    for(int j = 0; j < cols; j++) {
        int max = arr[0][j];   // Assume first row element is max
        
        for(int i = 1; i < rows; i++) {
            if(arr[i][j] > max) {
                max = arr[i][j];
            }
        }

        printf("Column %d: %d\n", j + 1, max);
    }

    return 0;
}
